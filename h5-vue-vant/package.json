    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "tailwind-merge": "^2.5.4",
    "vant": "^4.9.21",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.1.0",
    "@iconify-icons/fa6-solid": "^1.2.13",
    "@iconify/vue": "^4.1.2",
    "@tailwindcss/vite": "^4.1.16",
