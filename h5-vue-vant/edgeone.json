{
  "name": "dsj_h5",
  "buildCommand": "pnpm build-only",
  "installCommand": "pnpm i",
  "outputDirectory": "./dist",
  "nodeVersion": "22.17.1",

  "headers": [
    {
      "source": "/*.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "/assets/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
